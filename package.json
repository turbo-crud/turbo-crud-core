{
  "name": "fuckpress",
  "description": "fuckpress",
  "version": "1.0.0",
  "author": "JRichardsz <jrichardsz.java@gmail.com>",
  "license": "MIT",
  "main": "src/main/node/Index.js",
  "bin": {
    "fuckpress": "src/main/node/Index.js"
  },
  "dependencies": {
    "badges4cov": "github:jrichardsz/badges4cov",
    "chai": "^4.3.10",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "find-package-json": "^1.2.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "log4js": "^6.9.1",
    "lokijs": "^1.5.12",
    "markdown-it": "^14.0.0",
    "mocha": "^10.2.0",
    "mocha-suppress-logs": "^0.4.1",
    "mongoose": "^8.9.2",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "rimraf": "^6.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "dev_ini": "nodemon --watch src --watch theme  --watch webpage -e js,md,html,css --watch ./fp-admin.yaml --ignore foo.json src/main/node/Index.js --start",
    "dev": "nodemon --watch src --watch theme  --watch webpage -e js,md,html,css --watch ./fp-admin.yaml --ignore foo.json src/main/node/backend/Index.js --start",
    "publish": "node src/main/node/backend/Index.js --publish",
    "test": "nyc --reporter=html --reporter=json-summary mocha  'src/test/node/**/*.test.js' --exit --require mocha-suppress-logs && npm run badges",
    "test:verbose": "nyc --reporter=html --reporter=json-summary mocha  'src/test/node/**/*.test.js' --exit  --timeout 100000 && npm run badges",
    "badges": "badges4cov --source=nyc_json --nyc_json_file_location=./coverage/coverage-summary.json --output_folder=coverage"
  },
  "devDependencies": {
    "uuid": "^9.0.1"
  }
}
